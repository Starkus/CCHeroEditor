<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.MenuItem?>


<BorderPane fx:controller="net.starkus.cceditor.view.RootLayoutController" prefHeight="512.0" prefWidth="670.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="open" onAction="#handleLoad" mnemonicParsing="false" text="Open..." />
              <MenuItem fx:id="save" onAction="#handleSave" mnemonicParsing="false" text="Save" />
              <MenuItem fx:id="saveAs" onAction="#handleSaveAs" mnemonicParsing="false" text="Save as..." />
              <MenuItem fx:id="close" mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>